<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>plantsystem</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>CharsetFilter</display-name>
    <servlet-name>CharsetFilter</servlet-name>
    <servlet-class>controller.CharsetFilter</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CharsetFilter</servlet-name>
    <url-pattern>/charsetfilter</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>CharsetFilter</display-name>
    <filter-name>CharsetFilter</filter-name>
    <filter-class>controller.CharsetFilter</filter-class>
    <init-param>
      <param-name>charset</param-name>
      <param-value>utf-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>CharsetFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>LoginSevlet</display-name>
    <servlet-name>LoginSevlet</servlet-name>
    <servlet-class>controller.LoginSevlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginSevlet</servlet-name>
    <url-pattern>/myLogin</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>PlantServlet</display-name>
    <servlet-name>PlantServlet</servlet-name>
    <servlet-class>controller.PlantServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PlantServlet</servlet-name>
    <url-pattern>/plant</url-pattern>
  </servlet-mapping>
</web-app>